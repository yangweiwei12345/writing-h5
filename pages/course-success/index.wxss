page {
  height: 100%;
}
.container {
  min-height: 100%;
}

.success-img-box {
  margin-bottom: 30rpx;
  margin-top: 90rpx;
}
.success-img-box image {
  width: 100%;
  height: 100%;
  background-color: #e5e5e5;
}
.success-tip {
  display: inline;
  background-color: rgb(0, 133, 254);
  padding: 15rpx 30rpx;
  font-size: 13px;
  color: #fff;
  margin: 30rpx 80rpx;
  border-radius: 10rpx;
  position: relative;
}
.success-tip .success-icon {
  position: absolute;
  content: ' ';
  bottom: 0;
  left: -30rpx;
  width: 0;
  height: 0;
  border-bottom: 30rpx solid  rgb(0, 133, 254);
  border-left: 30px solid transparent;
}

.success-footer {
  display: flex;
  margin: 50rpx 40rpx;
}
.success-footer-btn-t {
  width: 320rpx !important;
}
.success-footer-btn {
  width: 30rpx;
  height: 30rpx;
  background-color: #e5e5e5;
  margin-right: 20rpx;
}