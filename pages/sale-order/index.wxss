page {
  height: 100%;
}
.container {
  min-height: 100%;
  background-color: #f6f6f6;
}

.search {
  margin: 0rpx 22rpx;
  display: flex;
}
.search-box {
  flex: 1;
  display: flex;
  background-color: #fff;
  padding: 0 26rpx;
  position: relative;

}

.select {
  position: absolute;
  top: 0;
  left: 0;
  z-index: 11;
  background-color: #fff;
  width: 100%;
  max-height: 600rpx;
  overflow-y: auto;
  margin-top: 86rpx;
  box-shadow: 0px 0px 5px 0px rgba(45, 45, 45, 0.2);
}
.select-item {
  height: 60rpx;
  display: flex;
  align-items: center;
  margin: 0 30rpx;
  font-size: 12px;
  font-family: Source Han Sans CN;
  font-weight: 400;
  color: #666666;
  border-bottom: 1px solid #eee;
}
.mask {
  position: fixed;
  top: 0;
  left: 0;
  z-index: 10;
  width: 100%;
  height: 100%;
  background-color: rgb(0, 0, 0, 0);
}

.search-value {
  flex: 1;
  display: flex;
  align-items: center;
  font-size: 12px;
  font-family: Source Han Sans CN;
  font-weight: 400;
  color: #999999;
}


.order-list {
  margin-top: 30rpx;
}
.order-item {
  background: #FFFFFF;
  box-shadow: 0px 0px 5px 0px rgba(45, 45, 45, 0.2);
  border-radius: 14rpx;
  padding: 20rpx 26rpx;
}
.order-head {
  display: flex;
  align-items: center;
  border-bottom: 1px solid #eee;
  padding-bottom: 22rpx;
}
.order-head-img {
  width: 60rpx;
  height: 60rpx;
  background-color: #e5e5e5;
  border-radius: 30rpx;
  margin-right: 20rpx;
}
.order-head-box {
  flex: 1;
}

.order-item-title {
  font-size: 12px;
  font-family: Source Han Sans CN;
  font-weight: 500;
  color: #333333;
}
.order-item-sec {
  font-size: 10px;
  font-family: Source Han Sans CN;
  font-weight: 400;
  color: #666666;
  display: flex;
}
.order-item-num {
  font-size: 9px;
  color: #999;
}
.order-head-price {
  font-size: 14px;
  font-family: Source Han Sans CN;
  font-weight: 400;
  color: #FF1F1F;
}


.order-content {
  margin-top: 16rpx;
}
.order-info-title {
  font-size: 12px;
  font-family: Source Han Sans CN;
  font-weight: 400;
  color: #333333;
}
.order-info-item {
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-top: 18rpx;

  font-size: 11px;
  font-family: Source Han Sans CN;
  font-weight: 400;
  color: #999999;
}
.order-info-label {}
.order-info-value {}
.order-footer {
  display: flex;
  align-items: center;
  margin-top: 16rpx;
}

.input {
  height: 30px;
}
.van-cell {
  padding: 3px 6px !important;
}
