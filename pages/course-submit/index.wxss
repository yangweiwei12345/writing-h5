}

.submit-page-img {
  margin-top: 30rpx;
}
.submit-page-img image {
  width: 90%;
  height: 100%;
  background-color: #e5e5e5;
}

.submit-param-sec {
  margin: 20rpx 0;
}
.submit-param-sec .submit-param-label {
  width: 700rpx;
  font-size: 16px;
