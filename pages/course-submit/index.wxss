page {
  height: 100%;
}
.container {
  min-height: 100%;
}

.submit-texts {
  height: 100%;
  position: relative;
  top: 200rpx;
}
.submit-text-item {
  font-size: 14px;
  color: #333;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-bottom: 80rpx;
}
.submit-text-item image {
  width: 30rpx;
  height: 25rpx;
  background-color: #e5e5e5;
  margin-right: 20rpx;
}

/* 提交页面 */
