page {
  height: 100%;
}
.container {
}


.rank-box {
}
.rank-box .rank-content {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: 16rpx;
  padding-bottom: 30rpx;
}

.rank-box .rank-content .rank-content-img {
  width: 550rpx;
  height: 80rpx;
}
.rank-box .rank-content-box {
  display: flex;
  width: 550rpx;
}

.rank-content-box-item {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: flex-end;
}
.rank-content-box-item .rank-content-box-item-text {
  font-size: 11px;
  font-family: SourceHanSansCN;
  font-weight: 400;
  color: #666666;
  text-align: center;
  height: 34rpx;
  overflow: hidden;
}

.rank-box .rank-content-box .rank-content-box-img{
  width: 154rpx;
  height: 170rpx;
  position: relative;
  display: flex;
  justify-content: center;
}
.rank-box .rank-content-box .rank-content-box-img-back{
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
}
.rank-box .rank-content-box .rank-content-box-img-back1 {
  width: 130rpx;
  height: 142rpx;
  left: 50%;
  margin-left: -65rpx;
}

.rank-box .rank-content-box .rank-content-box-img-head{
  width: 130rpx;
  height: 130rpx;
  border-radius: 65rpx;
  margin-left: 12rpx;
  margin-top: 30rpx;
  background-color: #e5e5e5;
}
.rank-box .rank-content-box .rank-content-box-img-head1{
  width: 100rpx;
  height: 100rpx;
  border-radius: 50rpx;
  margin-left: 0;
  margin-top: 26rpx;
}

.mb-56 {
  padding-top: 56rpx;
}

.rank-list {

}
.rank-list .rank-item {
  display: flex;
  align-items: center;
  height: 100rpx;
  margin: 0 60rpx;
  margin-bottom: 28rpx;
}
.rank-list .rank-item .rank-item-num {
  width: 60rpx;
  font-size: 18px;
  font-family: zcoolwenyiti;
  font-weight: 400;
  color: #666666;
}
.rank-list .rank-item .rank-item-img {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50rpx;
  margin-left: 36rpx;
  margin-right: 40rpx;
  background: #e5e5e5;
}
.rank-list .rank-item .rank-item-name {
  flex: 1;
  font-size: 11px;
  font-family: Source Han Sans CN;
  font-weight: 400;
  color: #666666;
}
.rank-list .rank-item .rank-item-like {
  font-size: 11px;
  font-family: Source Han Sans CN;
  font-weight: 400;
  color: #666666;
}
