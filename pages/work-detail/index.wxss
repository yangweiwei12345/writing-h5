page {
  height: 100%;
}
.container {
  min-height: 100%;
  background-color: #f6f6f6;
}

/** 头部 */
.container .header {
  position: relative;
  /* background-color: rgb(255, 144, 89); */
}

.header image {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 418rpx;
}

.container .content {
  height: 90rpx;
  text-align: center;
  line-height: 90rpx;
  font-size: 19px;
  font-family: SourceHanSansCN;
  font-weight: 500;
  color: #FFFFFF;
  position: relative;
  z-index: 10;
  
}
.header-back {
  position: absolute;
  top: 0;
  left: 0;
  width: 90rpx;
  height: 90rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.header-back image {
  width: 22rpx;
  height: 44rpx;
  margin-left: 34rpx;
  margin-top: 23rpx;
}

/** 顶部个人信息栏 */
.content-header {
  background: #FFFFFF;
  box-shadow: 0px 0px 4px 0px rgba(214, 5, 28, 0.2);
  border-radius: 7px;
  padding: 26rpx;
  margin: 28rpx;
  position: relative;
  z-index: 10;
}
.content-header-top {
  display: flex;
  align-items: center;
  border-bottom: 1px solid #f3f3f3;
}

.content-header-top .content-header-top-img {
  width: 150rpx;
  height: 164rpx;
  /* background-image: url('../../resource//my/header.png'); */
  background-size: 100%;
  background-repeat: no-repeat;
  display: flex;
  align-items: center;
  justify-content: center;
  position: relative;
}

.content-header-top-img-b {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
}

.content-header-top-img-t {
  width: 110rpx;
  height: 110rpx;
  border-radius: 55rpx;
  margin-bottom: 5rpx;

}

.content-header-top .content-header-top-box {
  display: flex;
  flex: 1;
  align-items: center;
  margin-left: 10rpx;
}
.content-header-top-info {
}
.content-header-top .content-header-top-box .header-box-left {
  display: flex;
  align-items: center;
  margin-bottom: 16rpx;
}
.header-box-left .header-box-left-name {
  font-size: 12px;
  font-family: SourceHanSansCN;
  font-weight: 500;
  color: #333333;
}
.header-box-left image {
  width: 24rpx;
  height: 24rpx;
  margin-left: 26rpx;
}

.content-header-top .content-header-top-box .content-header-top-info .header-box-right {
  display: flex;
  font-size: 10px;
  font-family: SourceHanSansCN;
  font-weight: 500;
  color: #999999;
}
.header-box-right .header-box-right-age {
  margin-right: 20rpx;
}
.header-box-right .header-box-right-level {}

.content-header-top .content-header-top-box > .header-box-right {
  width: 48rpx;
  height: 62rpx;
  margin-left: 40rpx;
}

.content-header-top .content-header-top-share {
  width: 120rpx;
  height: 120rpx;
}

.content-header-bottom {
  display: flex;
  align-items: center;
  justify-content: space-around;
  height: 240rpx;
}
.content-header-bottom-item {
  height: 100%;
  display: flex;
  flex-direction: column;
  flex: 1;
  align-items: center;
  justify-content: center;
}
.content-header-bottom-item image {
}

.content-header-bottom-item .image1 {
  width: 54rpx;
  height: 54rpx;
}

.content-header-bottom-item .image2 {
  width: 50rpx;
  height: 54rpx;
}

.content-header-bottom-item .image3 {
  width: 54rpx;
  height: 46rpx;
}

.content-header-bottom-item .image4 {
  width: 50rpx;
  height: 46rpx;
}
.content-header-bottom-item .content-header-bottom-item-title {
  font-size: 12px;
  font-family: SourceHanSansCN;
  font-weight: 400;
  color: #333333;
  margin-top: 18rpx;
  margin-bottom: 12rpx;
}
.content-header-bottom-item .content-header-bottom-item-num {
  font-size: 10px;
  font-family: SourceHanSansCN;
  font-weight: 400;
  color: #333333;
}


/** 作业列表 */

.work-container {
  background-color: #fff;
  box-shadow: 0px 0px 4px 0px rgba(78, 78, 78, 0.2);
}

.work-tab-class {
}
.work-tab-active-class {
  font-size: 14px;
  color: #EE1843;
}
.work-list {
  margin: 0 28rpx;
  padding-bottom: 180rpx;
}
.work-list-item {
  background: #FFFFFF;
  box-shadow: 0px 1px 7px 0px rgba(67, 65, 66, 0.2);
  border-radius: 3px;
  overflow: hidden;
  position: relative;
  margin-bottom: 30rpx;
}
.work-list-item .work-item-img {
  width: 100%;
  height: 338rpx;
  background-color: #e6e6ee;
}
.work-list-item .list-info {
  padding: 0 16rpx 16rpx 16rpx;
}
.work-list-item .list-info .list-info-title {
  font-size: 10px;
  font-family: SourceHanSansCN;
  font-weight: 400;
  color: #333333;
  line-height: 28rpx;
}
.work-list-item .list-info .list-info-box {
  display: flex;
  align-items: center;
  margin-top: 20rpx;
}
.work-list-item .list-info .list-info-box .list-info-text {
  font-size: 10px;
  font-family: SourceHanSansCN;
  font-weight: 400;
  color: #333333;
  margin: 0 12rpx;
}
.work-list-item .list-info .list-info-box .list-info-time {
  font-size: 10px;
  font-family: SourceHanSansCN;
  font-weight: 400;
  color: #999999;
}
.work-list-item .list-info .list-info-box image {
  width: 40rpx;
  height: 40rpx;
  border-radius: 40rpx;
  background-color: #e5e5e5;
}
.work-list-item .list-info-like {
  display: flex;
  align-items: center;
  height: 36rpx;
  border-radius: 36rpx;
  padding: 6rpx 12rpx;
  position: absolute;
  top: 18rpx;
  right: 20rpx;
  background-color: #f9f9f9;
}
.work-list-item .list-info-like image {
  width: 26rpx;
  height: 24rpx;
  margin-right: 6rpx;
}
.work-list-item .list-info-like .list-info-like-num {
  font-size: 10px;
  font-family: SourceHanSansCN;
  font-weight: 400;
  color: #666666;
}
.work-list-item .list-info-pj {
  width: 98rpx;
  height: 74rpx;
  position: absolute;
  right: 20rpx;
  top: 240rpx;
}

.work-remask {
  background: #FFFFFF;
  box-shadow: 0px 0px 5px 0px rgba(149, 13, 34, 0.2);
  border-radius: 7px;
  padding: 40rpx 36rpx;
  margin-right: 288rpx;
  margin-left: 28rpx;
  margin-bottom: 200rpx;
}

/* gif */
.work-gif {
  width: 244rpx;
  height: 244rpx;
  position: absolute;
  bottom: 160rpx;
  right: 20rpx;
  z-index: 11;
}
.work-gif image {
  width: 100%;
  height: 100%;
}

/* 送花 */
.work-hua {
  position: absolute;
  bottom: 160rpx;
  left: 20rpx;
  width: 62rpx;
  height: 58rpx;
  z-index: 11;
}
.work-hua image {
  width: 100%;
  height: 100%;
}
.work-hua .work-hua-num {
  background-color: rgba(76, 76, 76, .5);
  font-size: 12px;
  font-family: SourceHanSansCN;
  font-weight: 400;
  color: #FFFFFF;
  padding: 6rpx 8rpx;
  position: absolute;
  bottom: -16rpx;
  right: -25rpx;
  border-radius: 6rpx;
}


.footer {
  width: 100%;
  height: 140rpx;
  position: fixed;
  bottom: 0;
  left: 0;
  background-color: #fff;
  box-shadow: 0px -2px 2px 0px rgba(200, 200, 200, 0.3);
  display: flex;
  z-index: 11;

}
.footer-head {
  margin: 0 20rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.footer-head image {
  width: 90rpx;
  height: 90rpx;
  border-radius: 50rpx;
  background-color: #e5e5e5;
}
.footer-head-name {
  text-align: center;
  font-size: 12px;
  color: #999;
}

.footer-radio {
  flex: 1;
  display: flex;
  align-items: center;
}
.footer-radio .gp-common {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100%;
  height: 110rpx;
  border-radius: 55rpx;
}
/* 未点评状态 */
.footer-radio .gp-no {
  background-color: #aaa;
  font-size: 13px;
  color: #fff;
} 

/* 已点评，未播放状态 */
.gp-play, .gp-playing {
  display: flex;
  align-items: center;
  background-color: rgb(255, 0, 69);
}
.gp-play .gp-play-box {
  color: #fff;
  display: flex;
  flex: 1;
  flex-direction: column;
  justify-content: center;
  margin-left: 40rpx;
}
.gp-play .gp-play-box .gp-play-text {
  font-size: 15px;
  margin-bottom: 6rpx;
}
.gp-play .gp-play-box .gp-play-time {
  font-size: 10px;
}
.gp-play .gp-play-img {
  width: 80rpx;
  height: 80rpx;
  border-radius: 40rpx;
  margin-right: 20rpx;
}

/* 播放中 */
.gp-playing .gp-playing-img {
  width: 44rpx;
  height: 44rpx;
  margin-left: 18rpx;
  margin-right: 22rpx;
}
.gp-playing .gp-progress {
  display: flex;
  align-items: center;
  flex: 1;
  /* height: 1px; */
  /* background-color: rgb(255, 131, 159); */
  margin-right: 26rpx;
}
.gp-playing .gp-progress .gp-progress-line {
  height: 1px;
  position: relative;
  background-color: #fff;
}
.slider-custom-class {
  width: 282rpx;
}
.gp-playing .gp-progress .gp-progress-btn {
  /* position: absolute;
  top: -10rpx;
  right: -10rpx; */
  width: 20rpx;
  height: 20rpx;
  border-radius: 10rpx;
  background-color: #fff;
}
/*================*/

.footer-right {
  display: flex;
  margin: 0 20rpx;
}

.footer-share {
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.footer-share image {
  width: 80rpx;
  height: 80rpx;
  border-radius: 40rpx;
  background-color: #e5e5ee;
}
.footer-share button::after {
  border: none !important;
}
.footer-share .footer-share-text {
  text-align: center;
  font-size: 12px;
  color: #999;
}

.footer-like {
  display: flex;
  flex-direction: column;
  justify-content: center;
  margin-left: 20rpx;
}
.footer-like image {
  width: 80rpx;
  height: 80rpx;
  border-radius: 40rpx;
  background-color: #e5e5ee;
}
.footer-like .footer-like-text {
  text-align: center;
  font-size: 12px;
  color: #999;
}
